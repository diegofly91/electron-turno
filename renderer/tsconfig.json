{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "@/styles/*": ["styles/*"],
      "@/section/*": ["sections-pages/*"],
      "@/constant/*":["constant/*"],
      "@/components/*":["components/*"],
      "@/hooks/*":["hooks/*"],
      "@/pages/*":["pages/*"],
      "@/utils/*": ["utils/*"],
      "@/constants/*": ["constants/*"],
      "@/interfaces/*": ["interfaces/*"],
      "@/contexts/*": ["contexts/*"],
      "@/services/*": ["services/*"]

    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
